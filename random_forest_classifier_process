digraph {
	A [label=Start]
	B [label="Select Features and Target"]
	C [label="Handle Missing Values"]
	D [label="Convert Categorical Target to Numeric"]
	E [label="Split Data"]
	F [label="Train Random Forest Classifier"]
	G [label="Make Predictions"]
	H [label="Evaluate Model"]
